<div class="row" id="login-signup-row">
  <div class="col-md-6">
    <h2>Log In</h2>

    <form class="form login-form" id="log-in-form">
      <div class="form-group">
        <label for="email-login">Email:</label>
        <input class="form-input" type="text" id="email-login" />
      </div>
      <div class="form-group">
        <label for="password-login">Password:</label>
        <input class="form-input" type="password" id="password-login" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" id="login-page-btn" type="submit">Log In</button>
      </div>
    </form>
  </div>

  <div class="col-md-6">
    <h2>Sign Up</h2>
    <form class="form signup-form" id="sign-up-form">
      <div class="form-group">
        <label for="name-signup">Name:</label>
        <input class="form-input" type="text" id="name-signup" />
      </div>
      <div class="form-group">
        <label for="email-signup">Email:</label>
        <input class="form-input" type="text" id="email-signup" />
      </div>
      <div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
      <div class="avatar">
        <p class="select-avatar">Select Avatar:</p>
         <label>
          <input type="radio" name="avatar1" value="avatar-1">
          <img src="https://i.pinimg.com/280x280_RS/36/b0/6f/36b06fe18cf3cff94fb0febfc009dc3e.jpg"
            alt="Avatar" class="avatar">
        </label>
         <label>
          <input type="radio" name="avatar1" value="avatar-2">
          <img src="http://hildes.info/wp-content/uploads/2013/09/avatar_default-56a02492b24667c884f38ecb918666a3.png"
            alt="Avatar" class="avatar">
        </label>
        <label>
          <input type="radio" name="avatar1" value="avatar-3">
          <img
            src="data:image/jpeg;base64,"
            alt="Avatar" class="avatar">
        </label>
        <label>
          <input type="radio" name="avatar1" value="avatar-4">
          <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR7YhP4HnefYhoFHR4AZ-QXvstSMgjniE8LYw&usqp=CAU"
            alt="Avatar" class="avatar">
        </label>
        <label>
          <input type="radio" name="avatar1" value="avatar-5">
          <img
            src="data:image/jpeg;base64,"
            alt="Avatar" class="avatar">
        </label>
      </div>

      <div class="form-group">
        <button class="btn btn-primary" id="signup-page-btn" type="submit">Sign Up</button>
      </div>
    </form>
  </div>
</div>

<script src="./js/login.js"></script>