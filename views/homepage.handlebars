<!-- Loop over each object in the projects array -->
{{#each tracks as |track| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/tracks/{{track.id}}">{{track.title}}</a>
    </h2>
    <p>Created by {{tracks.user.name}} on {{format_date tracks.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{track.artist}}
    </p>
  </div>
</div>
{{/each}}
